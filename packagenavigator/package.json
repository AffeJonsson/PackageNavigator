{
  "name": "packagenavigator",
  "publisher": "alfredjonsson",
  "repository": "github:AffeJonsson/PackageNavigator",
  "displayName": "PackageNavigator",
  "description": "Allows you to navigate to external implementations when you have the source code locally available.",
  "version": "1.3.0",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "keybindings": [
      {
        "command": "packagenavigator.navigate",
        "key": "ctrl+alt+oem_period",
        "mac": "cmd+alt+oem_period"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "packagenavigator.navigate",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "packagenavigator.navigate",
        "title": "Navigate to local implementation"
      }
    ],
    "configuration": {
      "title": "PackageNavigator",
      "properties": {
        "packagenavigator.packages": {
          "type": [
            "array"
          ],
          "default": [],
          "description": "Test"
        }
      }
    }
  },
  "activationEvents": [
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  }
}
